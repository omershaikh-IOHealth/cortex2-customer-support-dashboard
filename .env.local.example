# ─────────────────────────────────────────────
# Database (required)
# ─────────────────────────────────────────────
DB_HOST=your_db_host
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# ─────────────────────────────────────────────
# Auth (NextAuth v5) — required
# Generate secret: openssl rand -base64 32
# ─────────────────────────────────────────────
AUTH_SECRET=your_random_32char_secret_here
NEXTAUTH_URL=http://localhost:3000

# ─────────────────────────────────────────────
# AI Companion (optional — disables AI chat if absent)
# ─────────────────────────────────────────────
CORE42_API_KEY=your_core42_api_key

# ─────────────────────────────────────────────
# n8n webhook (optional — disables Force Sync Now)
# ─────────────────────────────────────────────
N8N_WEBHOOK_URL=

# ─────────────────────────────────────────────
# ClickUp integration (optional — disables ticket push)
# ─────────────────────────────────────────────
CLICKUP_API_TOKEN=your_clickup_personal_api_token
CLICKUP_LIST_ID=your_clickup_list_id

# ─────────────────────────────────────────────
# ZIWO call centre (optional)
# ─────────────────────────────────────────────
ZIWO_CC_NAME=iohealth

# ─────────────────────────────────────────────
# Seed passwords (used by /api/setup migration)
# Override these in production — defaults are insecure placeholders
# ─────────────────────────────────────────────
SEED_ADMIN_PASSWORD=ChangeMe!Admin2025
SEED_AGENT_PASSWORD=ChangeMe!Agent2025
SEED_AGENT_ZIWO_PASSWORD=
